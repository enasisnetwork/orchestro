{
  "database": "sqlite:///_/encommon_sample/TMPPATH/_/db",
  "enconfig": {
    "paths": [
      "_/encommon_sample/PROJECT/_/examples",
      "_/encommon_sample/TMPPATH/_/orche"
    ]
  },
  "enlogger": {
    "stdo_level": "debug"
  },
  "dryrun": false,
  "groups": {
    "systems_almalinux": {
      "enable": true,
      "memberof": "systems",
      "display": "AlmaLinux"
    },
    "default": {
      "enable": false,
      "display": "Default Inventory"
    },
    "roles_enhomie": {
      "enable": true,
      "memberof": "default",
      "display": "Homie Automate",
      "ansible": {
        "enhomie_unique": "default",
        "enhomie_repository": "https://github.com/enasisnetwork/enhomie-conf",
        "enhomie_logging": true,
        "enhomie_console": false
      }
    },
    "roles_enrobie": {
      "enable": true,
      "memberof": "default",
      "display": "Chatting Robie",
      "ansible": {
        "enrobie_unique": "default",
        "enrobie_repository": "https://github.com/enasisnetwork/enrobie-conf",
        "enrobie_logging": true,
        "enrobie_console": false
      }
    },
    "jupiter": {
      "enable": true,
      "memberof": "default",
      "display": "Jupiter Inventory"
    },
    "neptune": {
      "enable": true,
      "memberof": "default",
      "display": "Neptune Inventory"
    },
    "systems_openbsd": {
      "enable": true,
      "memberof": "systems",
      "display": "OpenBSD Systems"
    },
    "systems": {
      "enable": true,
      "display": "Managed Systems"
    },
    "systems_windows": {
      "enable": true,
      "memberof": "systems",
      "display": "Windows"
    }
  },
  "persons": {
    "robert_domain": {
      "enable": true,
      "realm": "domain",
      "domain": "enasis.net",
      "first": "Robert",
      "last": "Harris"
    },
    "robert_local": {
      "enable": true,
      "realm": "local",
      "domain": "local",
      "first": "{{ source.persons.robert_domain.first }}",
      "last": "{{ source.persons.robert_domain.last }}"
    }
  },
  "subnets": {
    "jupiter_internal": {
      "enable": true,
      "subnet": "192.168.11.0/24",
      "memberof": "jupiter",
      "display": "Jupiter Internal"
    },
    "jupiter_domesnet": {
      "enable": true,
      "subnet": "192.168.51.0/24",
      "memberof": "jupiter",
      "display": "Jupiter Domesnet"
    },
    "jupiter_external": {
      "enable": true,
      "subnet": "192.168.91.0/24",
      "memberof": "jupiter",
      "display": "Jupiter External"
    },
    "neptune_internal": {
      "enable": true,
      "subnet": "192.168.12.0/24",
      "memberof": "neptune",
      "display": "Neptune Internal"
    },
    "neptune_domesnet": {
      "enable": true,
      "subnet": "192.168.52.0/24",
      "memberof": "neptune",
      "display": "Neptune Domesnet"
    },
    "neptune_external": {
      "enable": true,
      "subnet": "192.168.92.0/24",
      "memberof": "neptune",
      "display": "Neptune External"
    }
  },
  "systems": {
    "engwf1g1p": {
      "enable": true,
      "inherits": "template_openbsd",
      "memberof": "jupiter",
      "display": "Jupiter Gateway Primary"
    },
    "engwf1g2p": {
      "enable": true,
      "inherits": "template_openbsd",
      "memberof": "jupiter",
      "display": "Jupiter Gateway Secondary"
    },
    "engwf2g1p": {
      "enable": true,
      "inherits": "template_openbsd",
      "memberof": "neptune",
      "display": "Neptune Gateway Primary"
    },
    "engwf2g2p": {
      "enable": true,
      "inherits": "template_openbsd",
      "memberof": "neptune",
      "display": "Neptune Gateway Secondary"
    },
    "ensrv1p": {
      "enable": true,
      "inherits": "template_almalinux",
      "memberof": [
        "jupiter",
        "roles_enhomie"
      ],
      "display": "Jupiter Server",
      "ansible": {
        "enhomie_directory": "/opt/enhomie"
      }
    },
    "ensrv2p": {
      "enable": true,
      "inherits": "template_almalinux",
      "memberof": [
        "neptune",
        "roles_enrobie"
      ],
      "display": "Neptune Server",
      "ansible": {
        "enrobie_directory": "/opt/enrobie"
      }
    },
    "template_gateway": {
      "enable": false,
      "display": "Gateway Template"
    },
    "template_almalinux": {
      "enable": false,
      "inherits": "template_default",
      "memberof": "systems_almalinux",
      "display": "AlmaLinux Template"
    },
    "template_default": {
      "enable": false,
      "domain": "enasis.net",
      "display": "Default Template",
      "ansible": {
        "ansible_host": "{{ inventory_hostname }}",
        "ansible_user": "root"
      }
    },
    "template_openbsd": {
      "enable": false,
      "inherits": "template_default",
      "memberof": "systems_openbsd",
      "display": "OpenBSD Template"
    }
  }
}